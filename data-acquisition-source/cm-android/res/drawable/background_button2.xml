<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape android:shape="rectangle">
            <corners android:radius="5dp" />

            <stroke android:width="1dip" android:color="#90961A" />

            <gradient android:angle="180" android:endColor="#90961A" android:startColor="#90961A" />
        </shape></item>
    <item android:state_focused="true"><shape android:shape="rectangle">
            <corners android:radius="5dp" />

            <stroke android:width="1dip" android:color="#565912" />

            <solid android:color="#565912" />
        </shape></item>
    <item><shape android:shape="rectangle">
            <corners android:radius="5dp" />

            <stroke android:width="1dip" android:color="#565912" />

            <gradient android:angle="180" android:endColor="#565912" android:startColor="#565912" />
        </shape></item>

</selector>